{
  "name": "synthetics-sdk-mocha",
  "version": "1.0.0",
  "description": "NodeJS SDK for integrating with the Google Cloud Monitoring Synthetics product.",
  "main": "./build/src/index.js",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json ./test/**/*.spec.ts",
    "build": "rm -rf ./build && npm run compile",
    "compile": "tsc",
    "fix": "eslint --fix **/*.ts",
    "watch": "npm run compile -- --watch",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "lint": "eslint **/*.ts",
    "posttest": "npm run lint",
    "mocha": "mocha"
  },
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@google-cloud/functions-framework": "^3.1.3",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "@types/supertest": "^2.0.12",
    "chai": "^4.3.7",
    "supertest": "^6.3.3",
    "synthetics-sdk-mocha": "^1.0.0"
  },
  "dependencies": {
    "synthetics-sdk-api": "^1.0.0",
    "mocha": "^10.2.0"
  }
}

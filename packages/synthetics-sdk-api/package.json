{
  "name": "@google-cloud/synthetics-sdk-api",
  "version": "1.0.0",
  "description": "NodeJS distribution of the API Specification that must be complied with for integrating with Google Cloud Monitoring Synthetics",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "devDependencies": {},
  "scripts": {
    "build": "rm -rf ./build && npm run compile",
    "compile": "tsc",
    "fix": "eslint --fix **/*.ts",
    "watch": "npm run compile -- --watch",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "lint": "eslint **/*.ts",
    "regenerate_proto": "sh ./regenerate_proto_ts.sh"
  },
  "dependencies": {
    "ts-proto": "^1.143.0"
  },
